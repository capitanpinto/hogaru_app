<% if user_signed_in? %>
  <%= link_to "Mi Perfil", current_user, :class => 'navbar-link'  %> </br>
  <%= link_to "Edit", edit_user_registration_path %></br>
  <%= link_to "Log out", destroy_user_session_path, method: :delete, :class => 'navbar-link'  %> </br>
  <div class="row">
    <aside class="col-md-4">
      <section class="service_form">
        <%= render 'shared/service_form' %>
      </section>
    </aside>
  </div>
<%else%>
<%= link_to "Sign in", new_user_registration_path %></br>
<%= link_to "Log in", new_user_session_path, :class => 'navbar-link'  %> 
<%end%>
</div>